{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "interface-name": [true, "never-prefix"],
    "no-console": false,
    "no-shadowed-variable": [ true, {
      "underscore": false
    }],
    "semicolon": [ true, "never" ],
    "trailing-comma": false,
    "quotemark": [ true, "single" ]
  },
  "rulesDirectory": []
}
